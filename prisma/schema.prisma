// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Flashcard {
  id            String @id @default(uuid())
  category      String
  front         String
  back          String
  pronunciation String
}
